{
  "name": "@mrlvn/discord.js-framework",
  "version": "1.0.0",
  "description": "A library that makes it easy to handle commands and events using discord.js",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "deploy": "npm publish --access public",
    "deploy-dev": "npm publish --access public --tag dev",
    "test": "node --test"
  },
  "files": [
    "dist"
  ],
  "keywords": ["discord.js"],
  "author": "iMrlvn",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.0.0",
    "discord.js": "^14.20.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0"
  },
  "peerDependencies": {
    "discord.js": "^14"
  }
}