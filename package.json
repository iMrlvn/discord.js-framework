{
  "name": "mrlvn.js",
  "version": "0.0.1",
  "description": "A library that makes it easy to handle commands and events using discord.js",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./*": "./dist/*"
  },
  "scripts": {
    "dev": "tsc -w",
    "build": "tsc",
    "deploy": "npm publish --access public",
    "deploy-dev": "npm publish --access public --tag dev",
    "test": "node --test"
  },
  "files": [
    "dist"
  ],
  "keywords": [
      "discord.js",
      "framework",
      "handlers",
      "mrlvn.js"
  ],
  "author": "iMrlvn",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iMrlvn/mrlvn.js"
  },
  "bugs": {
    "url": "https://github.com/iMrlvn/mrlvn.js/issues"
  },
  "homepage": "https://github.com/iMrlvn/mrlvn.js#readme",
  "devDependencies": {
    "@types/node": "^24.0.10",
    "discord.js": "^14.21.0",
    "typescript": "^5.8.3"
  },
  "peerDependencies": {
    "discord.js": "^14"
  },
  "engines": {
    "node": ">=23.x"
  }
}