{
  "name": "mrlvn.js",
  "version": "0.0.1",
  "description": "A library that makes it easy to handle commands and events using discord.js",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "dev": "tsup lib/index.ts --format cjs,esm --dts --watch",
    "build": "tsup lib/index.ts --format cjs,esm --dts",
    "deploy": "npm publish --access public",
    "deploy-dev": "npm publish --access public --tag dev",
    "test": "node --test"
  },
  "files": [
    "dist/**/*"
  ],
  "keywords": ["discord.js"],
  "author": "iMrlvn",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iMrlvn/mrlvn.js"
  },
  "bugs": {
    "url": "https://github.com/iMrlvn/mrlvn.js/issues"
  },
  "homepage": "https://github.com/iMrlvn/mrlvn.js#readme",
  "devDependencies": {
    "@types/node": "^20.0.0",
    "discord.js": "^14.20.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0"
  },
  "peerDependencies": {
    "discord.js": "^14"
  }
}